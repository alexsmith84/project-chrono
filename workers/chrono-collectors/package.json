{
  "name": "chrono-collectors",
  "version": "1.0.0",
  "description": "Cloudflare Workers for collecting real-time cryptocurrency price data",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:production": "wrangler deploy --env production",
    "tail": "wrangler tail",
    "test:local": "~/.bun/bin/bun run test-local.ts",
    "test:coinbase": "EXCHANGE=coinbase ~/.bun/bin/bun run test-local.ts",
    "test:binance": "EXCHANGE=binance ~/.bun/bin/bun run test-local.ts",
    "test:kraken": "EXCHANGE=kraken ~/.bun/bin/bun run test-local.ts"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "cryptocurrency",
    "price-feeds"
  ],
  "author": "Project Chrono",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "typescript": "^5.7.2",
    "wrangler": "^3.94.0"
  }
}
